 <div ng-controller="InspectionTemplatesCtrl">
  <p id="notice"><%= notice %></p>

  <h1>Listing Inspection Templates</h1>

  <table>
    <thead>
      <tr>
        <th colspan="3"></th>
        <th>Name</th>
      </tr>
    </thead>

    <tbody>
        <tr ng-repeat="inspection_template in inspection_templates">
          <td>{{ inspection_template.name }}</td>
        </tr>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Inspection template', new_inspection_template_path %>
</div>