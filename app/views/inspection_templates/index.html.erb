<p id="notice"><%= notice %></p>

<h1>Listing Inspection Templates</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inspection_templates.each do |inspection_template| %>
      <tr>
        <td><%= link_to 'Show', inspection_template %></td>
        <td><%= link_to 'Edit', edit_inspection_template_path(inspection_template) %></td>
        <td><%= link_to 'Destroy', inspection_template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inspection template', new_inspection_template_path %>
